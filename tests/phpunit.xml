<?xml version="1.0"?>
<phpunit stopOnFailure="false" stopOnError="false">
    <php>
        <ini name="error_reporting" value="E_ALL &amp; ~(E_STRICT | E_NOTICE | E_DEPRACATED)"/>
    </php>
    <testsuite name="Test Suite">
        <directory suffix="Test.php">.</directory>
    </testsuite>
</phpunit>